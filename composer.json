{
  "name": "minutemanservices/wkhtmltopdf-php-client",
  "description": "PHP Client Library for generating PDFs from HTML using our Wkhtmltopdf Microservice",
  "type": "library",
  "license": "LGPL-3.0-only",
  "homepage": "https://bitbucket.org/minutemanservices/wkhtmltopdf-php-client",
  "authors": [
    {
      "name": "Tristan Hall",
      "email": "tristanh@mm.services"
    },
    {
      "name": "Malcolm Hall",
      "email": "malcolmh@mm.services"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MinuteMan\\WkhtmltopdfClient\\": "src/"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/support": "^8.0",
    "illuminate/validation": "^8.0",
    "konekt/enum": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinuteMan\\WkhtmltopdfClient\\WkhtmltopdfServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
