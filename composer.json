{
  "name": "minutemanservices/mms-wkhtmltopdf-php-client",
  "description": "PHP Client Library for generating PDFs from HTML using our Wkhtmltopdf Microservice",
  "type": "library",
  "license": "GNU AGPLv3",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Tristan Hall",
      "email": "tristanh@mm.services"
    },
    {
      "name": "Malcolm Hall",
      "email": "malcolmh@mm.services"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MinuteMan\\WkhtmltopdfClient\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4.0",
    "illuminate/support": ">=5.0.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinuteMan\\WkhtmltopdfClient\\WkhtmltopdfServiceProvider"
      ]
    }
  }
}
